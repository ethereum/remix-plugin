{
      "name": "example-theia-plugin",
      "publisher": "theia",
      "keywords": [
        "theia-plugin"
      ],
      "version": "0.3.0",
      "license": "none",
      
      "files": [
        "src"
      ],
      "activationEvents": [
        "*"
      ],
      "devDependencies": {
        "@theia/plugin": "next",
        "@theia/plugin-packager": "latest",
        "rimraf": "2.6.2",
        "typescript-formatter": "7.2.2",
        "typescript": "3.5.3"
      },
      "scripts": {
        "prepare": "yarn run clean && yarn run build",
        "clean": "rimraf lib",
        "format-code": "tsfmt -r",
        "watch": "tsc -watch",
        "compile": "tsc", 
        "build": "yarn run format-code && yarn run compile && theia-plugin pack"
      },
      "engines": {
        "theiaPlugin": "next"
      },
      "theiaPlugin": {
          
          
            "backend": "../../../dist/main.js"
          
      }
}
